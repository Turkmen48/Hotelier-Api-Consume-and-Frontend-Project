@model UpdateStaffViewModel
@{
    ViewData["Title"] = "UpdateStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form class="form-group" method="post">
    <input type="hidden" asp-for="StaffID" />
    <label>Personel Adı:</label>
    <input typeof="text" asp-for="Name" class="form-control"></>
    <br />
    <label>Personel Ünvanı:</label>
    <input typeof="text" asp-for="Title" class="form-control"></>
    <br />
    <label>Sosyal Medya 1:</label>
    <input typeof="text" asp-for="SocialMedia1" class="form-control"></>
    <br />
    <label>Sosyal Medya 2:</label>
    <input typeof="text" asp-for="SocialMedia2" class="form-control"></>
    <br />
    <label>Sosyal Medya 3:</label>
    <input typeof="text" asp-for="SocialMedia3" class="form-control"></>
    <br />
    <button type="submit" class="btn btn-warning">Güncelle</button>
</form>

