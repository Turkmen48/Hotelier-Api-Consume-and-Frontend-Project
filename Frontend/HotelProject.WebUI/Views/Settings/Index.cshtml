@model UserEditViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label>Adınız</label>
    <input type="text" class="form-control" asp-for="Name" />
    <br />
    <label>Soyadınız</label>
    <input type="text" class="form-control" asp-for="Surname" />
    <br />
    <label>Kullanıcı Adınız</label>
    <input type="text" class="form-control" asp-for="Username" readonly />
    <br />
    <label>Emailiniz</label>
    <input type="email" class="form-control" asp-for="Email" />
    <br />
    <label>Şifreniz</label>
    <input type="password" class="form-control" asp-for="Password" />
    <br />
    <label>Şifreniz Tekrar</label>
    <input type="password" class="form-control" asp-for="ConfirmPassword" />
    <br />
    <button type="submit" class="btn btn-success">Bilgileri Güncelle</button>
</form>

